		<!-- JUNIT 4 - https://mvnrepository.com/artifact/junit/junit -->
		<!-- <dependency> <groupId>junit</groupId> <artifactId>junit</artifactId> 
			<version>${junit.four.version}</version> <scope>test</scope> </dependency> -->

		<!-- JUNIT JUPITER - https://mvnrepository.com/artifact/org.junit.jupiter/junit-jupiter-api -->
		<!-- <dependency> <groupId>org.junit.jupiter</groupId> <artifactId>junit-jupiter-api</artifactId> 
			<version>${junit.jupiter.version}</version> <scope>test</scope> </dependency> -->


