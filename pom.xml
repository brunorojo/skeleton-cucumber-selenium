		<!-- REST-ASSURED - https://mvnrepository.com/artifact/io.rest-assured/rest-assured -->
		<!-- <dependency> <groupId>io.rest-assured</groupId> <artifactId>rest-assured</artifactId> 
			<version>${restassured.version}</version> </dependency> -->

